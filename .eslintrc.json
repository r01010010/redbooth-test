{
	"parser": "babel-eslint",
	"extends": "babel",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
	"rules":{
    "quotes": ["error", "single"],
    "comma-dangle": ["error", "never"],
    "no-underscore-dangle": 0,
    "arrow-parens": ["error", "as-needed"],
    "react/jsx-uses-vars": [2],
    "react/jsx-uses-react": 1
	},
	"env": {
	    "browser": true
	}
}
